#include <stdio.h>
int main()
{
    int k, j, i;
    int count = 0;
    printf("Enter two positive integers, k and j: ");
    scanf("%d%d", &k, &j);

    printf("Odd numbers between %d and %d are: \n", k, j);
    for (i = k; i <= j; i++) {
        if (i % 2 != 0) {
            printf("%d\n", i);
        } else {
            count++;
        }
    }

    printf("Number of even numbers between %d and %d: %d\n", k, j, count);
    return 0;
}
