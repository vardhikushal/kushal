#include <stdio.h>
int main()
{
    int m, n, i;
    int count = 0;
    printf("Enter two positive integers, M and N: ");
    scanf("%d%d", &m, &n);

    printf("Odd numbers between %d and %d are: \n", m, n);
    for (i = m; i <= n; i++) {
        if (i % 2 != 0) {
            printf("%d\n", i);
        } else {
            count++;
        }
    }

    printf("Number of even numbers between %d and %d: %d\n", m, n, count);
    return 0;
}

